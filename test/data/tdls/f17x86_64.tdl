<template>
  <name>f17jeos</name>
  <os>
    <name>Fedora</name>
    <version>17</version>
    <arch>x86_64</arch>
    <install type='url'>
      <url>http://download.fedoraproject.org/pub/fedora/linux/releases/17/Fedora/x86_64/os/</url>
    </install>
  </os>
  <description>Fedora 17</description>

  <packages>
    <package name="openssh-server"/>
  </packages>

  <files>
    <file name="/home/guest/.ssh/authorized_keys"><![CDATA[#!/bin/bash
ssh-rsa  guest@localhost.localdomain
]]>
    </file>
  </files>

  <commands>
    <command name="create_guest">useradd guest</command>
  </commands>
</template>
